let
	MAX ~ 10,
	num: Integer
in
	begin
		put("e"), put("n"), put("t"), put("e"), put("r"), put(" "),
		put("n"), put("u"), put("m"), put("b"), put("e"), put("r"), put(": "),
		getint(var num),
		if num > 0 then
			if num < MAX then
				while num > 0 do
					begin
						putint(num), puteol(),
						num becomes num-1
					end
			else nothing
		else nothing
	end
